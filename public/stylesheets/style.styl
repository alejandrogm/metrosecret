.container
	margin 0 auto
	padding-bottom 0.5em
	width 432px
	
.pull-left
	float left	
	
.row
	clear both
	overflow hidden
	text-align center
	
.span1
	float left
	height 99px
	width 22.92%
	
.span1:first-child
	margin-left 0
	
.span1
.span2
	margin-left 2.77%
	margin-top 2.66%
	
.span1.null-left	
.span2.null-left
	margin-left 0
	
.span2
	float left
	height 210px
	width 48.613%
	
.span2.span-right
	float right
	
.span2:first-child
	margin-left 0	
	
.span4
	height 210px
	margin-top 2.85%
	width 100%
	
